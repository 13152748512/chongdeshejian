page{
  position: relative;
  padding-bottom:170rpx;
}
.slide-image {
  width: 100%;
  height:100%;
}
swiper {
  height: 460rpx;
}
.notice{
  width: 100%;
  height: 80rpx;
  padding: 5rpx 5rpx 5rpx 0rpx;
  box-sizing: border-box;
  background: #fcd102;
  display: flex;
  justify-content: flex-start;
  line-height:80rpx;
}
.notice-img{
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
  position: relative;
  top: -7rpx;
  left: 8rpx;
}
.hint{
  width: 200rpx;
  text-align:center;
}
.title{
  color: #aa831a;
  font-size:32rpx;
}
.carousel{
  width: 73%;
  height:60rpx;
  font-size: 30rpx;
  color:#aa831a;
}
.moduleer{
  width: 100%;
  height: 160rpx;
  margin: 20rpx 0rpx;
  display: flex;
  justify-content: space-between;
  margin-top:40rpx;
}
.moduleer image{
  width: 80rpx;
  height: 80rpx;
}
.moduleer .child{
  width: 33.3%;
  text-align: center;
}
.child view{
  margin-top: 20rpx;
  font-size: 30rpx;
  color: #999;
}
.introduce{
  width: 100%;
